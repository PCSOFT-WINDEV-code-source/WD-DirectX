   -
     name : D3DInit
     internal_properties : CAAAAAgAAABsimSHDPYiQ9BUKZ7+oNnGUSFcLbSEHRbof1kMNVyze889UUM/rtam9f2CiUMaYul20rQSiydI6xKjIgBKcgFAQV3TEpVXoVYhybnhA2aNVoNkU62+l0yySJ0/PKVOhmZlLa2DiL1pkTLo0++PekM5U7Jfthj5vPBO1gXS0H3Bh+hSV7YH0cWG2cw/Us0Ysl9ObhSGObNppgIz2vAmSqfIuzwNx1DGw67M0dbqcXsBIMZEw2qc0dryYbG/V5bhGw==
     procedure_id : 1867989089630267516
     type_code : 14
     code : |1-
      // Résumé : Initialise Direct3D
      Procédure D3DInit()
      
      	stD3DPP 		est un D3DPRESENT_PARAMETERS
      		
      	// Création de l'objet Direct3D
      	gpclD3D = Direct3DCreate9(D3D_SDK_VERSION)
      	
      	// Vérifie si l'objet Direct3D a pu être créé
      	SI gpclD3D = Null ALORS
      		RENVOYER Faux	
      	FIN
      	
      	// Change la résolution de l'écran
      	SysChangeRésolutionEcran(800, 600, pD3DUtils.gstResolution:bBitsPerPixel, pD3DUtils.gstResolution:RefreshRate)
      			
      	// Modifie les dimensions et la position de la fenêtre
      	mafenetre..x = 0
      	mafenetre..Y = 0
      	mafenetre..Largeur = 800
      	mafenetre..hauteur = 600
      	
      	// Passe la fenêtre en avant-plan
      	MaFenêtre..Visible = vrai
      	fenetat(MaFenêtre, actifdevant)
      	fenetat(MaFenêtre, dessustout)
      	
      	// Paramétrage de la structure utilisée pour créer un "device" Direct3D
      	stD3DPP:Windowed = faux														// Mode plein écran
      	stD3DPP:hDeviceWindow = handle(MaFenêtre)									// C'est le champ image qui recevra les informations
      	stD3DPP:BackBufferFormat = D3DFMT_X8R8G8B8									// 32bits
      	stD3DPP:BackBufferWidth = 800												// Largeur de l'écran
      	stD3DPP:BackBufferHeight = 600												// Hauteur de l'écran
      	stD3DPP:BackBufferCount = 1							
      	stD3DPP:SwapEffect = D3DSWAPEFFECT_DISCARD							
      	stD3DPP:FullScreen_RefreshRateInHz = pD3DUtils.gstResolution:RefreshRate									
      	
      	// Création du "device" Direct3D
      	// On utilise ici l'écran par défaut (D3DADAPTER_DEFAULT) et un "device" matériel (D3DDEVTYPE_HAL)
      	si failed(gpclD3D:CreateDevice(D3DADAPTER_DEFAULT, D3DDEVTYPE_HAL, handle(mafenetre), D3DCREATE_HARDWARE_VERTEXPROCESSING, stD3DPP, gpclD3DDevice)) _ou_ gpclD3DDevice = null ALORS
      		renvoyer faux
      	FIN
      	
      renvoyer vrai
