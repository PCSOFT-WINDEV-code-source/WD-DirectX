     internal_properties : CAAAAAgAAAAu58UGYTaqq0VAg+y3uvRhOpypq7r2386+NtQ9NSrU5bZQT1JK+BIAd7ExZ2aArpvZUlizMLNY9qjNF9sn/jiTBhyE5b2945vmpgXnJLUiQYdDETKBnXlWNI7t1pChuqUd3jpOUd0PLPS7hRBsaJzxInWeR94J2fGUTsHl49vWjp/YNe4zba5PsNPjJkENuftq
     code : |1+
      
      //
      // Cette fenêtre permet de créer un "device" Direct3D
      //
      
      // Objets
      gpclD3D				est un IDirect3D9 dynamique				// Objet de base permettant de créer un "device"
      gpclD3DDevice 		est un IDirect3DDevice9 dynamique		// Objet de manipulation du "device"
   -
     code : |1-
      
      // Pour fermer la fenêtre, il faudra appuyer sur ALT+F4
      si pas EnModeTestAutomatique alors info("Pour terminer, appuyez sur ALT+F4")
      
      // Initialisation de Direct3D
      si D3DInit() ALORS
      	
      	// En plein écran, il est conseillé de cacher le curseur de la souris
      	CurseurAffiche(faux)
      	
      	// Démarre le rendu
      	D3DAffiche()
      sinon
      	
      	SI EnModeTestAutomatique ALORS
      		TestEcritRésultat(terInfo, "DirectX n'est pas disponible sur cette machine")
      		Ferme
      		RETOUR
      	FIN
      	
      	erreur("Impossible d'initialiser Direct3D")
      	ferme()
      FIN
