     code : |1-
      // Résumé : Initialise le texte
      Procédure D3DInitTexte()
      
      	hDC est un entier système
      	hFont est un entier système
      	hFontOld est un entier système
      	nScale est un entier
      	nHauteur est un entier
      
      	// Création d'un objet CD3DFONT
      	gpclTexte = allouer un CD3DFont("Arial")
      	si gpcltexte = null ALORS
      		RENVOYER Faux
      	FIN
      		
      	// Initialise l'objet
      	si failed(gpclTexte:InitDeviceObjects(gpclD3DDevice)) ALORS
      		renvoyer faux
      	FIN
      		
      	SI FAILED(gpclTexte:RestoreDeviceObjects()) ALORS
      		RENVOYER Faux
      	fin
      	
      	// Création d'un DC
      	hDC 	 = API("GDI32", "CreateCompatibleDC", Null)	
      	
      	// Calcul de la hauteur de la police
      	nScale   = SysMétrique(90)
      	nHauteur = -((9 * nScale) / 72)
      	
      	// Crée la police
      	hFont	 = API("GDI32", "CreateFontA", nHauteur, 0, 0, 0, 700, faux, Faux, Faux, 1, 0, 0, 1, 2, "Arial")
      	SI hFont = Null ALORS 
      		RENVOYER faux
      	FIN
      	
      	// Sélectionne la police
      	hFontOld = API("GDI32", "SelectObject", hDC, hFont)
      	
      	// Crée le texte 3D
      	si failed(D3DXCreateTextA(gpclD3DDevice, hDC, "Texte 3D", 0.001, 0.4, gpclDXTexte, null, null)) _OU_ gpclDXTexte = Null ALORS
      		renvoyer faux
      	FIN
      	
      	// Nettoyage
      	API("GDI32", "SelectObject", hDC, hFontOld) 
      	API("GDI32", "DeleteObject", hFont) 
      	API("GDI32", "DeleteDC",   hDC) 
      	
      	// Récupère les dimensions du texte 3D
      	D3DXGetMeshDimensions(gpclD3DDevice, gpclDXTexte, grLargTx, grHautTx, null)
      	
      	// Crée une police	
      	SI FAILED(D3DXCreateFontA(gpclD3DDevice, 12, 0, 400, 1, Faux, 0, 0, 1, 2, "Lucida Console", gpclDXFont)) _OU_ gpclDXFont = Null ALORS
      		RENVOYER Faux
      	FIN
      		
      renvoyer vrai
