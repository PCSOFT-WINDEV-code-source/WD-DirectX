     name : D3DAffiche
     internal_properties : 
     procedure_id : 1868395221701665175
     type_code : 14
     code : |1-
      // Résumé : Rendu et affichage du "device" Direct3D
      // Procédure automatique :
      // La procédure est exécutée manuellement, lors d'un appel dans le code
      // Elle sera répétée en boucle, en attendant  entre chaque appel
      // Chaque appel suivant exécute une seule fois la procédure, sans timer
      // 
      Procédure D3DAffiche()
      
      	stMaterial est un D3DMATERIAL9 //dynamique = allouer un D3DMATERIAL9
      
      	// Si le "device" Direct3D est invalide, on sort
      	si gpclD3DDevice = null alors retour
      	si gpclTexte = null alors retour
      	
      	// Efface le "backbuffer" avec une couleur bleue
      	gpclD3DDevice:Clear(0, null, D3DCLEAR_TARGET | D3DCLEAR_ZBUFFER, D3DCOLOR_XRGB(0, 0, 255), 1.0, 0.0)
      	
      	// Démarre la scène
      	si SUCCEEDED(gpclD3DDevice:BeginScene()) ALORS
      		
      		// ID3DXFont
      		gpclDXFont:DrawTextA(null, "Texte avec ID3DXFont", -1, make_rect(320, 0, img_dx..largeur, img_dx..hauteur / 2), 0x00000004, D3DCOLOR_ARGB(255, 255, 255, 255))
      				
      		// Texte agrandi x2 (0.03 * 2)
      		gpclTexte:DrawTextScaled(0.0, -0.5, 0.0, 0.06, 0.06,  D3DCOLOR_XRGB(0, 0, 0), "Texte noir, agrandi x2", 	CD3DFont::D3DFONT_CENTERED_X 									|	CD3DFont::D3DFONT_FILTERED	)
      		
      		// Texte multiligne
      		gpclTexte:DrawText(0.0, 70.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Texte sur" + RC + "deux lignes",										 									 	CD3DFont::D3DFONT_FILTERED	)
      
      		// Texte fixe
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Haut gauche", 						CD3DFont::D3DFONT_top 											| 	CD3DFont::D3DFONT_FILTERED	)
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Haut droite", 							CD3DFont::D3DFONT_top 			| CD3DFont::D3DFONT_right		| 	CD3DFont::D3DFONT_FILTERED	)
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Bas gauche", 						CD3DFont::D3DFONT_bottom 										| 	CD3DFont::D3DFONT_FILTERED	)
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Bas droite", 							CD3DFont::D3DFONT_BOTTOM 		| CD3DFont::D3DFONT_RIGHT		| 	CD3DFont::D3DFONT_FILTERED	)
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Haut centré", 						CD3DFont::D3DFONT_Centered_x 	| CD3DFont::D3DFONT_TOP			|	CD3DFont::D3DFONT_FILTERED	)
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Gauche centré",	 					CD3DFont::D3DFONT_Centered_y 									| 	CD3DFont::D3DFONT_FILTERED	)
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Droite centré",	 					CD3DFont::D3DFONT_Centered_y 	| CD3DFont::D3DFONT_RIGHT		| 	CD3DFont::D3DFONT_FILTERED	)
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Centré", 							CD3DFont::D3DFONT_Centered_y 	| CD3DFont::D3DFONT_CENTERED_X 	| 	CD3DFont::D3DFONT_FILTERED	)
      		gpclTexte:DrawText( 0.0, 0.0, 0.0, D3DCOLOR_XRGB(255, 255, 255), "Bas centré", 							CD3DFont::D3DFONT_Centered_x 	| CD3DFont::D3DFONT_bottom	 	| 	CD3DFont::D3DFONT_FILTERED	)
      				
      		// Initialisation des matrices
      		D3DInitMatrices()
      				
      		// "Material" du texte 3D
      		stMaterial:Diffuse:red 		= 0.0; stMaterial:Ambient:red 		= 0.0
      		stMaterial:Diffuse:green 	= 1.0; stMaterial:Ambient:green 	= 0.5
      		stMaterial:Diffuse:blue 	= 0.0; stMaterial:Ambient:blue 		= 0.0
      		stMaterial:Diffuse:alpha 	= 1.0; stMaterial:Ambient:alpha 	= 1.0
      		gpclD3DDevice:SetMaterial(stMaterial)
      				
      		// Dessin du texte 3D
      		gpclDXTexte:DrawSubset(0)
      		
      		// Fin de la scène
      		gpclD3DDevice:EndScene()
      	FIN
      	
      	// Affichage du "backbuffer"
      	gpclD3DDevice:Present(null, null, null, null)
